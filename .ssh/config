# HostName aliases
Host pi
    HostName 192.168.178.25
Host home
    HostName sbh.duckdns.org
Host dm
    HostName dm-server.informatik.uni-bremen.de
Host lina
    HostName lina.informatik.uni-bremen.de

# Multiplexing
Host dm lina
    ControlPath ~/.ssh/cm-%r@%h:%p
    ControlMaster auto
    ControlPersist 10m

# Identities
Host pi home
    IdentityFile ~/.ssh/id_rsa
Host dm lina gitlab.informatik.uni-bremen.de
    IdentityFile ~/.ssh/ub
    User hoeffner

