# HostName aliases
Host dm
    HostName dm-server.informatik.uni-bremen.de
Host lina
    HostName lina.informatik.uni-bremen.de
Host dwight
    HostName 134.102.211.127
Host litmus
    HostName litmus.informatik.uni-bremen.de

# Multiplexing
Host *
    ControlPath ~/.ssh/cm-%r@%h:%p
    ControlMaster auto
    ControlPersist 10m

# Identities
Host litmus litmus.informatik.uni-bremen.de
    IdentityFile ~/.ssh/ub
    User root

Host *.informatik.uni-bremen.de
    IdentityFile ~/.ssh/ub
    User hoeffner
