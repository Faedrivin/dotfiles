# HostName aliases
Host scruffy
    HostName 134.102.211.143
Host dm
    HostName dm-server.informatik.uni-bremen.de
Host lina
    HostName lina.informatik.uni-bremen.de
Host dwight
    HostName 134.102.211.127

# Multiplexing
Host dm lina dwight scruffy
    ControlPath ~/.ssh/cm-%r@%h:%p
    ControlMaster auto
    ControlPersist 10m

# Identities
Host scruffy home
    IdentityFile ~/.ssh/id_rsa
Host dm lina gitlab.informatik.uni-bremen.de
    IdentityFile ~/.ssh/ub
    User hoeffner

