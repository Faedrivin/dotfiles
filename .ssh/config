Host ikw gate.ikw.uos.de
    HostName gate.ikw.uos.de
    ControlPath ~/.ssh/cm-%r@%h:%p
    ControlMaster auto
    ControlPersist 10m
    ForwardAgent yes

Host cippy* twilight shadow
    ProxyCommand ssh %r@ikw -W %h:%p

Host tb
    ProxyCommand ssh -T -L 5003:localhost:5003 twilight make tb

Host kill
    ProxyCommand ssh -T shoeffner@gate.ikw.uos.de kill -HUP -1
